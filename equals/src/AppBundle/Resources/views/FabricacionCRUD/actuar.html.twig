{% if object.estado == 'inconsistente' %}
	<a class="btn btn-sm btn-warning" href="{{ admin.generateObjectUrl('completar', object) }}"><b>Completar</b></a>
{% elseif object.estado == 'pendiente' %}
	<a class="btn btn-sm btn-success" href="{{ admin.generateObjectUrl('confirmar', object) }}"><b>Confirmar </b></a>
	<a class="btn btn-sm btn-info" href="{{ admin.generateObjectUrl('imprimirOrden', object) }}"><b>Orden</b></a>
	<a class="btn btn-sm btn-info" href="{{ admin.generateObjectUrl('imprimirEtiqueta', object) }}"><b>Etiqueta</b></a>
	<a class="btn btn-sm btn-info" href="{{ admin.generateObjectUrl('imprimirCoa', object) }}"><b>COA</b></a>
{% elseif object.estado == 'fabricado' %}
	<a class="btn btn-sm btn-info" href="{{ admin.generateObjectUrl('imprimirOrden', object) }}"><b>Orden </b></a>
	<a class="btn btn-sm btn-info" href="{{ admin.generateObjectUrl('imprimirEtiqueta', object) }}"><b>Etiqueta</b></a>
	<a class="btn btn-sm btn-info" href="{{ admin.generateObjectUrl('imprimirCoa', object) }}"><b>COA</b></a>
{% endif %}

